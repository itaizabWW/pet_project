<h1>Store#index</h1>

    <table>
      <thead>
      <tr>
        <th>Name</th>
        <th>Cuisine</th>
        <th>Rating</th>
        <th>10bis</th>
        <th>Address</th>
        <th>Price</th>
        <th>Maxdevtime</th>
        <th>Review</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @restaurants.each do |restaurant| %>

          <tr>

            <td><%= restaurant.name %></td>
            <td><%= restaurant.cuisine_id %></td>
            <td><%= restaurant.rating %></td>
            <td><%= restaurant.tenbis %></td>
            <td><%= restaurant.address %></td>
            <td><%= restaurant.price %></td>
            <td><%= restaurant.maxDevTime %></td>



            <% @reviews.each do |review| %>
                <% if review.restaurant_id   == restaurant.id %>
                    <td><%= review.comment %></td>
                <% end %>
            <% end %>



            <td><%= link_to 'Add a review', new_review_path(restaurant_id: restaurant.id) %></td>

            <!--<td><%= link_to 'Show', restaurant %></td>-->
            <!--<td><%= link_to 'Edit', edit_restaurant_path(restaurant) %></td>-->
            <!--<td><%= link_to 'Destroy', restaurant, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
          </tr>
      <% end %>
      </tbody>
    </table>



